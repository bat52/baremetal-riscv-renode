sysbus:
    init:
        Tag <0xE0000800 0x200> "UART_PHY"

mem: Memory.MappedMemory @ sysbus 0x0
    size: 0x00040000

cpu: CPU.VexRiscv @ sysbus

timer: Timers.LiteX_Timer @ sysbus 0x60002800
    frequency: 100000000
    IRQ -> cpu@1

uart: UART.LiteX_UART @ sysbus 0x60001800
    IRQ -> cpu@2
